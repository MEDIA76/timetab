{
  "name": "Timetab",
  "version": "0.1",
  "description": "Simply displays the time, or times, whenever a new tab is opened.",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "New Tab"
  },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "manifest_version": 2
}