{
  "name": "TimeTab",
  "version": "0.2.0",
  "description": "Simply displays the time, or times, whenever a new tab is opened.",
  "author": "Joshua Britt",
  "homepage_url": "https://github.com/capachow/timetab",
  "permissions": ["storage"],
  "icons": {
    "128": "images/icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "New Tab"
  },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "manifest_version": 2
}